#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i, term;
	printf("Enter the number of terms");
	fflush(stdout);
	scanf("%d",&term);
	for(i=9;i==i*10+9;++i){
		printf("%d\n",&i);
	}

	return EXIT_SUCCESS;
}
