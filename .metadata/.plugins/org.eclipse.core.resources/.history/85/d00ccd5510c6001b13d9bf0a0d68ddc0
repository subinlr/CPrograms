#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,limit,value[100],searchvkey;
	printf("Enter Limit");
	fflush(stdout);
	scanf("%d",&limit);
	printf("Enter Values");
	for(i=0;i<limit;i++){
		scanf("%d",value[i])
	}

	return EXIT_SUCCESS;
}
