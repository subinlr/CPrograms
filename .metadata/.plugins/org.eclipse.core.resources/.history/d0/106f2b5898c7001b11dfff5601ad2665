#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,limit,i,j,k,l,temp,value[100];
	printf("Enter Limit");
	scanf("%d",&limit);
	printf("Enter Values of Array1");
	for(i=0;i<limit;i++){
		scanf("%d",&value[i]);
	}
		for(i=0;i<limit-1;i++){
			for(j=i+1;j<limit;j++){
				if(value[i]>value[j]){
					temp=value[i];
					value[i]=value[j];
					value[j]=temp;
				}
			}
		}
		printf("Enter Values of Array2");
		for(i=0;i<limit;i++){
			scanf("%d",&value[k]);
		}

		for(l=0;l<limit-1;l++){
			(k=l+1;K<limit;k++){

			}

		}
	return EXIT_SUCCESS;
}
